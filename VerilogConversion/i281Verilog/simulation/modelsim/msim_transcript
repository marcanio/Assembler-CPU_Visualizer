sim:/_programCounter/vcc_signal
run
force -freeze sim:/_programCounter/Control 1 0
force -freeze sim:/_programCounter/PC_Input 000001 0
force -freeze sim:/_programCounter/Clock 1 0, 0 {50 ps} -r 100
force -freeze sim:/_programCounter/Reset 0 0
run
force -freeze sim:/_programCounter/PC_Input 000011 0
run
run
